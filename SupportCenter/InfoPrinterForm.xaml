<Window x:Class="SupportCenter.InfoPrinterForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SupportCenter"
        mc:Ignorable="d"
        Title="InfoPrinterForm" Height="469" Width="800" Loaded="Window_Loaded">
    <Window.Resources>
        <ResourceDictionary Source="/Styles/Style.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Image Source="/Image/Main.jpg" StretchDirection="UpOnly" Stretch="Fill"/>
        <Grid Background="#EF3C3636" Opacity="1" HorizontalAlignment="Center" Width="800" VerticalAlignment="Center">
            <TextBlock Margin="197,10,0,552" TextWrapping="Wrap" FontSize="16" Foreground="White" FontFamily="Yu Gothic UI Semibold" Text="Создание обращения" HorizontalAlignment="Left" Width="167">
                <TextBlock.Effect>
                    <DropShadowEffect/>
                </TextBlock.Effect>
            </TextBlock>
            <StackPanel Margin="0,8,0,0" Background="#C53C3636" Orientation="Horizontal" Opacity="1" HorizontalAlignment="Center" Width="800" Height="50" VerticalAlignment="Top">
                <Button x:Name="HP430Button" Style="{StaticResource ButtonMenuStyle}" Margin="10,0,0,0" Height="50" Width="60" Cursor="Hand" Click="HP430Button_Click">
                    <Button.Effect>
                        <DropShadowEffect/>
                    </Button.Effect>
                    <TextBlock TextWrapping="Wrap" TextAlignment="Center" Text="HP 430"/>
                </Button>
                <Button x:Name="HP428Button" Style="{StaticResource ButtonMenuStyle}" Height="50" Margin="5,0,0,0" Width="60" Cursor="Hand" Click="HP428Button_Click">
                    <Button.Effect>
                        <DropShadowEffect/>
                    </Button.Effect>
                    <TextBlock TextWrapping="Wrap" TextAlignment="Center" Text="HP 428"/>
                </Button>
                <Button x:Name="HP426Button" Style="{StaticResource ButtonMenuStyle}" Margin="5,0,0,0" Height="50" Width="60" Cursor="Hand" Click="HP426Button_Click">
                    <Button.Effect>
                        <DropShadowEffect/>
                    </Button.Effect>
                    <TextBlock TextWrapping="Wrap" TextAlignment="Center" Text="HP 426"/>
                </Button>
            </StackPanel>
            <TabControl x:Name="printersTabControl" Margin="0,73,0,0" BorderBrush="{x:Null}" Background="{x:Null}" Height="364" VerticalAlignment="Top" HorizontalAlignment="Center" Width="780">
                <TabItem x:Name="infoTabItem" Header="HP 430" BorderBrush="{x:Null}" Background="White">
                    <Grid Margin="0,0,4,0" Background="#C53C3636">
                        <TextBlock TextWrapping="Wrap" Margin="10,20,0,0" Foreground="White" Height="22" VerticalAlignment="Top" HorizontalAlignment="Left" Width="308"><Run Language="ru-ru" Text="Модель принтера указана с передней части принтера."/></TextBlock>
                        <TextBlock TextWrapping="Wrap" Margin="410,20,0,0" Foreground="White" Text="IP адрес принтера можно узнать нажав на значок вопросительного знака справа в углу, в открывшемся меню выбрать поле Ethernet, пример IP ардеса 192.168.1.1" Height="52" VerticalAlignment="Top" HorizontalAlignment="Left" Width="350"/>
                        <Image Margin="410,77,22,39" Source="/ImagePrinter/HP430 IP.png" Stretch="Fill"/>
                        <Image Margin="10,47,443,18" Source="/ImagePrinter/HP 430 Model.png" Stretch="Fill"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="infoTabItem_Копировать" Header="HP 428" BorderBrush="{x:Null}" Background="White" Margin="-2,-2,-2,0">
                    <Grid Margin="0,0,4,0" Background="#C53C3636">
                        <TextBlock TextWrapping="Wrap" Margin="10,20,0,0" Foreground="White" Height="22" VerticalAlignment="Top" HorizontalAlignment="Left" Width="308"><Run Language="ru-ru" Text="Модель принтера указана с передней части принтера."/></TextBlock>
                        <TextBlock TextWrapping="Wrap" Margin="410,20,0,0" Foreground="White" Height="52" VerticalAlignment="Top" HorizontalAlignment="Left" Width="350"><Run Text="IP адрес принтера можно узнать нажав на значок "/><Run Language="ru-ru" Text="указанный на картинке, там ниже будет указан "/><Run Text="IP "/><Run Language="ru-ru" Text="адрес в формате "/><Run Text="192.168.1.1"/></TextBlock>
                        <Image Margin="410,77,37,21" Source="/ImagePrinter/HP428 IP.png" Stretch="Fill"/>
                        <Image Margin="10,46,411,17" Source="/ImagePrinter/HP 428 Model.png" Stretch="Fill"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="infoTabItem_Копировать1" Header="HP 426" BorderBrush="{x:Null}" Background="White" Margin="-2,-2,-2,0">
                    <Grid Margin="0,0,4,0" Background="#C53C3636">
                        <TextBlock TextWrapping="Wrap" Margin="10,20,0,0" Foreground="White" Height="22" VerticalAlignment="Top" HorizontalAlignment="Left" Width="308"><Run Language="ru-ru" Text="Модель принтера указана с передней части принтера."/></TextBlock>
                        <TextBlock TextWrapping="Wrap" Margin="410,20,0,0" Foreground="White" Text="IP адрес принтера можно узнать нажав на значок вопросительного знака справа в углу, в открывшемся меню выбрать поле Ethernet, пример IP ардеса 192.168.1.1" Height="52" VerticalAlignment="Top" HorizontalAlignment="Left" Width="350"/>
                        <Image Margin="410,80,19,28" Source="/ImagePrinter/HP426IP.png" Stretch="Fill"/>
                        <Image Margin="10,41,435,10" Source="/ImagePrinter/HP426Model.png" Stretch="Fill"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="infoTabItem_Копировать2" Header="HP 430" BorderBrush="{x:Null}" Background="White" Margin="-2,-2,-2,0">
                    <Grid Margin="0,0,4,0" Background="#C53C3636">
                        <TextBlock TextWrapping="Wrap" Margin="10,20,0,0" Foreground="White" Height="22" VerticalAlignment="Top" HorizontalAlignment="Left" Width="308"><Run Language="ru-ru" Text="Модель принтера указана с передней части принтера."/></TextBlock>
                        <Image Margin="10,56,0,0" Source="/ImagePrinter/Screenshot_5.png" Stretch="Fill" Height="253" VerticalAlignment="Top" HorizontalAlignment="Left" Width="296"/>
                        <TextBlock TextWrapping="Wrap" Margin="410,20,0,0" Foreground="White" Text="IP адрес принтера можно узнать нажав на значок вопросительного знака справа в углу, в открывшемся меню выбрать поле Ethernet, пример IP ардеса 192.168.1.1" Height="52" VerticalAlignment="Top" HorizontalAlignment="Left" Width="350"/>
                        <Image Margin="410,92,0,0" Source="/ImagePrinter/Screenshot_6.png" Stretch="Fill" Height="217" VerticalAlignment="Top" HorizontalAlignment="Left" Width="342"/>
                    </Grid>
                </TabItem>
            </TabControl>
        </Grid>
    </Grid>
</Window>
